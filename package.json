{
    "name": "tetris",
    "version": "0.0.0",
    "description": "Tetris clone.",
    "author": "Frederick Gnodtke",
    "license": "MIT",
    "scripts": {
        "build": "webpack -p",
        "start": "webpack-dev-server",
        "test": "TZ=UTC jest",
        "lint": "tslint --format stylish --project tsconfig.json"
    },
    "main": "./index.html",
    "jest": {
        "preset": "ts-jest",
        "collectCoverage": true,
        "moduleDirectories": [
            "node_modules",
            "src",
            "assets",
            "."
        ],
        "moduleFileExtensions": [
            "ts",
            "js"
        ],
        "transform": {
            "\\.ts": "ts-jest"
        },
        "coverageReporters": [
            "lcov",
            "text-summary"
        ],
        "testMatch": [
            "**/__tests__/test-*.ts"
        ],
        "globals": {
            "ts-jest": {
                "diagnostics": true
            }
        },
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/__tests__/"
        ],
        "collectCoverageFrom": [
            "src/**/*.ts"
        ],
        "setupFilesAfterEnv": [
            "./jest-setup.ts"
        ],
        "reporters": [
            "default",
            "jest-screenshot/reporter"
        ]
    },
    "devDependencies": {
        "@types/classnames": "^2.2.7",
        "@types/date-fns": "^2.6.0",
        "@types/jest": "^25.2.1",
        "@types/node": "14.0.5",
        "@types/ramda": "^0.27.6",
        "@types/random-seed": "^0.3.3",
        "@types/react": "^16.8.3",
        "@types/react-router": "^4.4.5",
        "@types/react-router-dom": "^4.3.1",
        "@types/recharts": "^1.1.16",
        "@types/uuid": "^8.0.0",
        "canvas": "^2.6.1",
        "classnames": "^2.2.6",
        "css-loader": "^3.5.3",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^6.0.0",
        "git-revision-webpack-plugin": "^3.0.3",
        "jest": "^26.0.1",
        "jest-screenshot": "^0.3.1",
        "json-loader": "^0.5.7",
        "node-sass": "^4.11.0",
        "resolve-url-loader": "^3.0.1",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.2.1",
        "ts-jest": "^26.0.0",
        "ts-loader": "^7.0.5",
        "tslint": "^6.1.2",
        "typescript": "^3.3.3",
        "url-loader": "^4.1.0",
        "webpack": "^4.29.5",
        "webpack-cli": "^3.2.3",
        "webpack-dev-server": "^3.1.14"
    },
    "dependencies": {
        "date-fns": "^2.14.0",
        "lodash-decorators": "^6.0.1",
        "mobx": "^5.9.0",
        "mobx-react": "^6.2.2",
        "peerjs": "^1.2.0",
        "ramda": "^0.27.0",
        "random-seed": "^0.3.0",
        "react": "^16.8.2",
        "react-dom": "^16.8.2",
        "react-router": "^5.0.0",
        "react-router-dom": "^5.0.0",
        "react-semantic-ui-range": "^0.7.0",
        "recharts": "^1.5.0",
        "semantic-ui-css": "^2.4.1",
        "semantic-ui-react": "^0.88.2",
        "tsdi": "^0.22.0",
        "uuid": "^8.1.0"
    }
}
